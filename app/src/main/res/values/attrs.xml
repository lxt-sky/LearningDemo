<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgressView">
        <attr name="mBorderWidth" format="dimension"/>
        <attr name="mBorderColor" format="color"/>
        <attr name="mRadiusColor" format="color"/>
        <attr name="mArcColor" format="color"/>
        <attr name="mTextColor" format="color"/>
        <attr name="mTextSize" format="dimension"/>
        <attr name="mStokeCap">
            <enum name="SQUARE" value="0"/>
            <enum name="BUTT" value="1"/>
            <enum name="ROUND" value="2"/>
        </attr>
        <attr name="mStartPosition">
            <enum name="left" value="180"/>
            <enum name="top" value="-90"/>
            <enum name="right" value="0"/>
            <enum name="bottom" value="90"/>
        </attr>
    </declare-styleable>
</resources>